{
  "name": "pocuo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "supabase:start": "~/.local/bin/supabase start",
    "supabase:stop": "~/.local/bin/supabase stop",
    "supabase:status": "~/.local/bin/supabase status",
    "supabase:reset": "~/.local/bin/supabase db reset",
    "test": "npm run supabase:start && npx jest && npm run supabase:stop",
    "test:watch": "npx jest --watch",
    "test:coverage": "npm run supabase:start && npx jest --coverage && npm run supabase:stop",
    "test:ci": "npm run supabase:start && npx jest --ci --coverage --watchAll=false && npm run supabase:stop",
    "test:db": "npm run supabase:start && npx jest --testPathPatterns=integration && npm run supabase:stop"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "next": "15.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "autoprefixer": "^10.0.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.5.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  },
  "overrides": {
    "test-exclude": "7.0.1"
  }
}
